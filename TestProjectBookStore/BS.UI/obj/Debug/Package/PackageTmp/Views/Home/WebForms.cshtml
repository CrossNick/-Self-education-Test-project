@{
    ViewBag.Title = "Home Page";
}
<script>
    function resizeIframe(obj) {
        obj.style.height = obj.contentWindow.document.body.scrollHeight + 'px';
    }
</script>
<iframe width="100%" height="100%" frameborder="0" scrolling="no" onload="resizeIframe(this)" src="@System.Configuration.ConfigurationManager.AppSettings["WebForms"]" name="iframe_a"></iframe>