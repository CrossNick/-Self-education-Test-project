{
  "compileOnSave": true,
  "compilerOptions": {
    "module": "umd",
    "target": "es6",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": [ "es2015", "dom" ],
    "noImplicitAny": false,
    "suppressImplicitAnyIndexErrors": true,
    "inlineSourceMap": true,
    "inlineSources": true,
    "outDir": "./AutoGenerated"
  },
  "exclude": [
    "./node_modules"
  ]
}
